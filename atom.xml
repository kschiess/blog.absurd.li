<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>publish: Kaspar Schiess</title>
 <link href="http://blog.absurd.li/atom.xml" rel="self"/>
 <link href="http://blog.absurd.li/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blog.absurd.li/</id>
 <author>
   <name>Kaspar Schiess</name>
   <email>kaspar@absurd.li</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://tom.preston-werner.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://tom.preston-werner.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>